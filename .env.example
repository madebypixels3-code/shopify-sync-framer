# .env.example - Template for environment variables
# Copy to .env.local and fill in your own values

# Lemon Squeezy API Key for license validation
# Get your API key from: https://app.lemonsqueezy.com/settings/api
LEMON_SQUEEZY_API_KEY=your_lemon_squeezy_api_key_here

# Port for backend server (default: 3001)
PORT=3001

# Environment (development or production)
NODE_ENV=development
